<?xml version="1.0" encoding="utf-8"?>
<config>
  <!--清洗-->
  <DisinfectMonitorSeries>
    <MonitorSeries>
      <FName>CT</FName>
      <FMax>180</FMax>
      <FMin>0</FMin>
      <FColor>Blue</FColor>
      <FGridSpacing>20</FGridSpacing>
      <FUnit>℃</FUnit>
      <FYName>Temperature</FYName>
      <FDataIndex>2</FDataIndex>
    </MonitorSeries>
    <MonitorSeries>
      <FName>AO</FName>
      <FMax>4500</FMax>
      <FMin>0</FMin>
      <FColor>Green</FColor>
      <FGridSpacing>500</FGridSpacing>
      <FUnit></FUnit>
      <FYName>AO</FYName>
      <FDataIndex>4</FDataIndex>
    </MonitorSeries>
  </DisinfectMonitorSeries>
  <!--灭菌-->
  <SterilizeMonitorSeries>
    <MonitorSeries>
      <FName>温度</FName>
      <FMax>140</FMax>
      <FMin>0</FMin>
      <FColor>Blue</FColor>
      <FGridSpacing>20</FGridSpacing>
      <FUnit>℃</FUnit>
      <FYName>Temperature</FYName>
      <FDataIndex>2</FDataIndex>
    </MonitorSeries>
    <MonitorSeries>
      <FName>压力</FName>
      <FMax>3500</FMax>
      <FMin>0</FMin>
      <FColor>Green</FColor>
      <FGridSpacing>500</FGridSpacing>
      <FUnit>mbar a</FUnit>
      <FYName>Pressure</FYName>
      <FDataIndex>4</FDataIndex>
    </MonitorSeries>
  </SterilizeMonitorSeries>
  <!--环境-->
  <EnvironmentMonitorSeries>
    <MonitorSeries>
      <FName>温度</FName>
      <FMax>50</FMax>
      <FMin>0</FMin>
      <FColor>Blue</FColor>
      <FGridSpacing>15</FGridSpacing>
      <FUnit>℃</FUnit>
      <FYName>Temperature</FYName>
    </MonitorSeries>
    <MonitorSeries>
      <FName>湿度</FName>
      <FMax>100</FMax>
      <FMin>0</FMin>
      <FColor>Green</FColor>
      <FGridSpacing>15</FGridSpacing>
      <FUnit>%</FUnit>
      <FYName>
      </FYName>
    </MonitorSeries>
  </EnvironmentMonitorSeries>
  <!--实时数据预估时长(分钟)-->
  <Estimate value="60">
  </Estimate>
  <!--取数延时时间(秒)-->
  <GetDataInterval value="5">
  </GetDataInterval>
  <!--窗体循环显示间隔时间(秒)-->
  <RefreshTime value="10">
  </RefreshTime>
  <!--开启循环刷新显示-->
  <OpenRefresh value="0">
  </OpenRefresh>
  <!--需要检测的字段序号-->
  <NeedCheckDataIndex value="1" />
  <Devices>
    <MonitorDevice>
      <DeviceName>1号灭菌器</DeviceName>
      <DeviceCode>11501</DeviceCode>
      <DeviceType>4</DeviceType>
      <ShowType>1</ShowType>
    </MonitorDevice>
    <MonitorDevice>
      <DeviceName>2号灭菌器</DeviceName>
      <DeviceCode>11502</DeviceCode>
      <DeviceType>4</DeviceType>
      <ShowType>1</ShowType>
    </MonitorDevice>
    <MonitorDevice>
      <DeviceName>3号灭菌器</DeviceName>
      <DeviceCode>11503</DeviceCode>
      <DeviceType>4</DeviceType>
      <ShowType>1</ShowType>
    </MonitorDevice>
    <MonitorDevice>
      <DeviceName>4号灭菌器</DeviceName>
      <DeviceCode>11504</DeviceCode>
      <DeviceType>4</DeviceType>
      <ShowType>1</ShowType>
    </MonitorDevice>

    <MonitorDevice>
      <DeviceName>1号清洗器</DeviceName>
      <DeviceCode>11101</DeviceCode>
      <DeviceType>0</DeviceType>
      <ShowType>2</ShowType>
    </MonitorDevice>
    <MonitorDevice>
      <DeviceName>2号清洗器</DeviceName>
      <DeviceCode>11104</DeviceCode>
      <DeviceType>0</DeviceType>
      <ShowType>1</ShowType>
    </MonitorDevice>
    <MonitorDevice>
      <DeviceName>3号清洗器</DeviceName>
      <DeviceCode>11105</DeviceCode>
      <DeviceType>0</DeviceType>
      <ShowType>1</ShowType>
    </MonitorDevice>
    <MonitorDevice>
      <DeviceName>4号清洗器</DeviceName>
      <DeviceCode>11106</DeviceCode>
      <DeviceType>0</DeviceType>
      <ShowType>1</ShowType>
    </MonitorDevice>
  </Devices>
</config>
