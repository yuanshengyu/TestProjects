<?xml version="1.0" encoding="utf-8"?>
<config>
  <!--清洗-->
  <DisinfectMonitorSeries>
    <MonitorSeries>
      <FName>温度</FName>
      <FMax>150</FMax>
      <FMin>0</FMin>
      <FColor>Blue</FColor>
      <FGridSpacing>15</FGridSpacing>
      <FUnit>℃</FUnit>
      <FYName>Temperature</FYName>
      <FDataIndex>2</FDataIndex>
    </MonitorSeries>
    <MonitorSeries>
      <FName>湿度</FName>
      <FMax>100</FMax>
      <FMin>0</FMin>
      <FColor>Green</FColor>
      <FGridSpacing>15</FGridSpacing>
      <FUnit>%</FUnit>
      <FYName>
      </FYName>
      <FDataIndex>2</FDataIndex>
    </MonitorSeries>
  </DisinfectMonitorSeries>
  <!--灭菌-->
  <SterilizeMonitorSeries>
    <MonitorSeries>
      <FName>内温</FName>
      <FMax>150</FMax>
      <FMin>0</FMin>
      <FColor>Red</FColor>
      <FGridSpacing>15</FGridSpacing>
      <FUnit>℃</FUnit>
      <FYName>Temperature</FYName>
      <FDataIndex>1</FDataIndex>
    </MonitorSeries>
    <MonitorSeries>
      <FName>外温</FName>
      <FMax>100</FMax>
      <FMin>0</FMin>
      <FColor>Blue</FColor>
      <FGridSpacing>15</FGridSpacing>
      <FUnit>℃</FUnit>
      <FYName>Temperature</FYName>
      <FDataIndex>2</FDataIndex>
    </MonitorSeries>
    <MonitorSeries>
      <FName>压力</FName>
      <FMax>3</FMax>
      <FMin>-3</FMin>
      <FColor>Green</FColor>
      <FGridSpacing>1</FGridSpacing>
      <FUnit>Mp</FUnit>
      <FYName>Pressure</FYName>
      <FDataIndex>3</FDataIndex>
    </MonitorSeries>
  </SterilizeMonitorSeries>
  <!--环境-->
  <EnvironmentMonitorSeries>
    <MonitorSeries>
      <FName>温度</FName>
      <FMax>50</FMax>
      <FMin>0</FMin>
      <FColor>Blue</FColor>
      <FGridSpacing>15</FGridSpacing>
      <FUnit>℃</FUnit>
      <FYName>Temperature</FYName>
    </MonitorSeries>
    <MonitorSeries>
      <FName>湿度</FName>
      <FMax>100</FMax>
      <FMin>0</FMin>
      <FColor>Green</FColor>
      <FGridSpacing>15</FGridSpacing>
      <FUnit>%</FUnit>
      <FYName>
      </FYName>
    </MonitorSeries>
  </EnvironmentMonitorSeries>
  <!--实时数据预估时长(分钟)-->
  <Estimate value="60">
  </Estimate>
  <!--取数延时时间(秒)-->
  <GetDataInterval value="5">
  </GetDataInterval>
  <!--窗体循环显示间隔时间(秒)-->
  <RefreshTime value="10">
  </RefreshTime>
  <!--开启循环刷新显示-->
  <OpenRefresh value="1">
  </OpenRefresh>
  <!--需要检测的字段序号-->
  <NeedCheckDataIndex value="1" />
  <Devices>
    <MonitorDevice>
      <DeviceName>1号压力蒸汽灭菌器</DeviceName>
      <DeviceCode>11507</DeviceCode>
      <DeviceType>4</DeviceType>
    </MonitorDevice>
    <MonitorDevice>
      <DeviceName>2号压力蒸汽灭菌器</DeviceName>
      <DeviceCode>11508</DeviceCode>
      <DeviceType>4</DeviceType>
    </MonitorDevice>
    <MonitorDevice>
      <DeviceName>3号压力蒸汽灭菌器</DeviceName>
      <DeviceCode>11509</DeviceCode>
      <DeviceType>4</DeviceType>
    </MonitorDevice>
    <MonitorDevice>
      <DeviceName>回收</DeviceName>
      <DeviceCode>0000A732</DeviceCode>
      <DeviceType>8</DeviceType>
    </MonitorDevice>
    <MonitorDevice>
      <DeviceName>配包</DeviceName>
      <DeviceCode>0000A733</DeviceCode>
      <DeviceType>8</DeviceType>
    </MonitorDevice>
    <MonitorDevice>
      <DeviceName>灭菌</DeviceName>
      <DeviceCode>0000A734</DeviceCode>
      <DeviceType>8</DeviceType>
    </MonitorDevice>
    <MonitorDevice>
      <DeviceName>发放</DeviceName>
      <DeviceCode>000074B5</DeviceCode>
      <DeviceType>8</DeviceType>
    </MonitorDevice>
    <MonitorDevice>
      <DeviceName>1号消毒清洗机</DeviceName>
      <DeviceCode></DeviceCode>
      <DeviceType>0</DeviceType>
      <Url><![CDATA[www.baidu.com]]></Url>
    </MonitorDevice>
    <MonitorDevice>
      <DeviceName>2号消毒清洗机</DeviceName>
      <DeviceCode></DeviceCode>
      <DeviceType>0</DeviceType>
      <Url><![CDATA[www.baidu.com]]></Url>
    </MonitorDevice>
    <MonitorDevice>
      <DeviceName>3号消毒清洗机</DeviceName>
      <DeviceCode></DeviceCode>
      <DeviceType>0</DeviceType>
      <Url><![CDATA[localhost:8081/SmartDeviceService/]]></Url>
    </MonitorDevice>
    <MonitorDevice>
      <DeviceName>4号消毒清洗机</DeviceName>
      <DeviceCode></DeviceCode>
      <DeviceType>0</DeviceType>
      <Url><![CDATA[localhost:8081/SmartDeviceService/]]></Url>
    </MonitorDevice>
  </Devices>
</config>
