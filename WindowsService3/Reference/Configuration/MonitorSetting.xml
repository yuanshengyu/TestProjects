<?xml version="1.0" encoding="utf-8"?>
<config>
  <!--清洗-->
  <DisinfectMonitorSeries>
    <MonitorSeries>
      <FName>温度</FName>
      <FMax>180</FMax>
      <FMin>0</FMin>
      <FColor>Blue</FColor>
      <FGridSpacing>20</FGridSpacing>
      <FUnit>℃</FUnit>
      <FYName>Temperature</FYName>
      <FDataIndex>2</FDataIndex>
      <FVisible>true</FVisible>
      <FGridVisible>true</FGridVisible>
      <FormatString>N1</FormatString>
    </MonitorSeries>
    <MonitorSeries>
      <FName>A0</FName>
      <FMax>5000</FMax>
      <FMin>0</FMin>
      <FColor>Green</FColor>
      <FGridSpacing>500</FGridSpacing>
      <FUnit></FUnit>
      <FYName>AO</FYName>
      <FDataIndex>4</FDataIndex>
      <FVisible>true</FVisible>
      <FGridVisible>true</FGridVisible>
      <FormatString></FormatString>
    </MonitorSeries>

    <MonitorSeries>
      <FName>媒价编号</FName>
      <FMax>0</FMax>
      <FMin>0</FMin>
      <FColor>Green</FColor>
      <FGridSpacing>0</FGridSpacing>
      <FUnit></FUnit>
      <FYName>AO</FYName>
      <FDataIndex>1</FDataIndex>
      <FVisible>false</FVisible>
      <FGridVisible>true</FGridVisible>
      <FormatString></FormatString>
    </MonitorSeries>
    <MonitorSeries>
      <FName>泵编号</FName>
      <FMax>0</FMax>
      <FMin>0</FMin>
      <FColor>Green</FColor>
      <FGridSpacing>0</FGridSpacing>
      <FUnit></FUnit>
      <FYName>AO</FYName>
      <FDataIndex>3</FDataIndex>
      <FVisible>false</FVisible>
      <FGridVisible>true</FGridVisible>
      <FormatString></FormatString>
    </MonitorSeries>
    <MonitorSeries>
      <FName>泵送量</FName>
      <FMax>0</FMax>
      <FMin>0</FMin>
      <FColor>Green</FColor>
      <FGridSpacing>0</FGridSpacing>
      <FUnit></FUnit>
      <FYName>AO</FYName>
      <FDataIndex>5</FDataIndex>
      <FVisible>false</FVisible>
      <FGridVisible>true</FGridVisible>
      <FormatString></FormatString>
    </MonitorSeries>
  </DisinfectMonitorSeries>
  <!--灭菌-->
  <SterilizeMonitorSeries>
    <MonitorSeries>
      <FName>温度</FName>
      <FMax>180</FMax>
      <FMin>0</FMin>
      <FColor>Blue</FColor>
      <FGridSpacing>20</FGridSpacing>
      <FUnit>℃</FUnit>
      <FYName>Temperature</FYName>
      <FDataIndex>2</FDataIndex>
      <FVisible>true</FVisible>
      <FGridVisible>true</FGridVisible>
      <FormatString>N1</FormatString>
    </MonitorSeries>
    <MonitorSeries>
      <FName>压力</FName>
      <FMax>3500</FMax>
      <FMin>0</FMin>
      <FColor>Green</FColor>
      <FGridSpacing>500</FGridSpacing>
      <FUnit>mbar a</FUnit>
      <FYName>Pressure</FYName>
      <FDataIndex>4</FDataIndex>
      <FVisible>true</FVisible>
      <FGridVisible>true</FGridVisible>
      <FormatString></FormatString>
    </MonitorSeries>

    <MonitorSeries>
      <FName>夹套压力</FName>
      <FMax>3500</FMax>
      <FMin>0</FMin>
      <FColor>Green</FColor>
      <FGridSpacing>500</FGridSpacing>
      <FUnit>mbar a</FUnit>
      <FYName>Pressure</FYName>
      <FDataIndex>5</FDataIndex>
      <FVisible>false</FVisible>
      <FGridVisible>true</FGridVisible>
      <FormatString></FormatString>
    </MonitorSeries>
    <MonitorSeries>
      <FName>蒸汽压力</FName>
      <FMax>6000</FMax>
      <FMin>0</FMin>
      <FColor>Green</FColor>
      <FGridSpacing>500</FGridSpacing>
      <FUnit>mbar a</FUnit>
      <FYName>Pressure</FYName>
      <FDataIndex>6</FDataIndex>
      <FVisible>false</FVisible>
      <FGridVisible>true</FGridVisible>
      <FormatString></FormatString>
    </MonitorSeries>
  </SterilizeMonitorSeries>
  <!--环境-->
  <EnvironmentMonitorSeries>
    <MonitorSeries>
      <FName>温度</FName>
      <FMax>50</FMax>
      <FMin>0</FMin>
      <FColor>Blue</FColor>
      <FGridSpacing>15</FGridSpacing>
      <FUnit>℃</FUnit>
      <FYName>Temperature</FYName>
    </MonitorSeries>
    <MonitorSeries>
      <FName>湿度</FName>
      <FMax>100</FMax>
      <FMin>0</FMin>
      <FColor>Green</FColor>
      <FGridSpacing>15</FGridSpacing>
      <FUnit>%</FUnit>
      <FYName>
      </FYName>
    </MonitorSeries>
  </EnvironmentMonitorSeries>
  <!--实时数据预估时长(分钟)-->
  <Estimate value="60">
  </Estimate>
  <!--取数延时时间(秒)-->
  <GetDataInterval value="15">
  </GetDataInterval>
  <!--窗体循环显示间隔时间(秒)-->
  <RefreshTime value="20">
  </RefreshTime>
  <!--开启循环刷新显示-->
  <OpenRefresh value="1">
  </OpenRefresh>
  <!--需要检测的字段序号-->
  <NeedCheckDataIndex value="1" />
  <!--曲线图数据点-->
  <ChartPoint value="5"></ChartPoint>
  <Devices>
    <MonitorDevice>
      <DeviceName>1号灭菌器</DeviceName>
      <DeviceCode>11501</DeviceCode>
      <DeviceType>4</DeviceType>
      <ShowType>1</ShowType>
    </MonitorDevice>
    <MonitorDevice>
      <DeviceName>2号灭菌器</DeviceName>
      <DeviceCode>11502</DeviceCode>
      <DeviceType>4</DeviceType>
      <ShowType>1</ShowType>
    </MonitorDevice>
    <MonitorDevice>
      <DeviceName>3号灭菌器</DeviceName>
      <DeviceCode>11503</DeviceCode>
      <DeviceType>4</DeviceType>
      <ShowType>1</ShowType>
    </MonitorDevice>
    <MonitorDevice>
      <DeviceName>4号灭菌器</DeviceName>
      <DeviceCode>11504</DeviceCode>
      <DeviceType>4</DeviceType>
      <ShowType>1</ShowType>
    </MonitorDevice>
	  <MonitorDevice>
      <DeviceName>5号灭菌器</DeviceName>
      <DeviceCode>11505</DeviceCode>
      <DeviceType>4</DeviceType>
      <ShowType>1</ShowType>
    </MonitorDevice>

    <MonitorDevice>
      <DeviceName>1号清洗器</DeviceName>
      <DeviceCode>11101</DeviceCode>
      <DeviceType>0</DeviceType>
      <ShowType>1</ShowType>
    </MonitorDevice>
    <MonitorDevice>
      <DeviceName>2号清洗器</DeviceName>
      <DeviceCode>11102</DeviceCode>
      <DeviceType>0</DeviceType>
      <ShowType>1</ShowType>
    </MonitorDevice>
	<MonitorDevice>
      <DeviceName>3号清洗器</DeviceName>
      <DeviceCode>11103</DeviceCode>
      <DeviceType>0</DeviceType>
      <ShowType>1</ShowType>
    </MonitorDevice>
    
  </Devices>
  
  <!--曲线打印配置-->
  <CurvePrintConfig>
    <CurveInfo>
      <Width>900</Width>
      <Height>800</Height>
      <Margin>50</Margin>
      <DeviceType>4</DeviceType>
      <SeriesX>
        <SeriesInfo>
          <MaxValue>80</MaxValue>
          <MinValue>0</MinValue>
          <Align>Bottom</Align>
          <GridSpace>2</GridSpace>
          <MarkGridSpace>10</MarkGridSpace>
          <DataIndex>0</DataIndex>
        </SeriesInfo>
      </SeriesX>
      <SeriesY>
        <SeriesInfo>
          <Name>温度</Name>
          <Unit>℃</Unit>
          <LineColor>Blue</LineColor>
          <MaxValue>180</MaxValue>
          <MinValue>0</MinValue>
          <Align>Left</Align>
          <GridSpace>2</GridSpace>
          <MarkGridSpace>20</MarkGridSpace>
          <DataIndex>2</DataIndex>
        </SeriesInfo>
        <SeriesInfo>
          <Name>压力</Name>
          <Unit>mbar a</Unit>
          <LineColor>Green</LineColor>
          <MaxValue>3500</MaxValue>
          <MinValue>0</MinValue>
          <Align>Right</Align>
          <GridSpace>50</GridSpace>
          <MarkGridSpace>500</MarkGridSpace>
          <DataIndex>4</DataIndex>
        </SeriesInfo>
      </SeriesY>
    </CurveInfo>
    <CurveInfo>
      <Width>900</Width>
      <Height>800</Height>
      <Margin>50</Margin>
      <DeviceType>0</DeviceType>
      <SeriesX>
        <SeriesInfo>
          <MaxValue>60</MaxValue>
          <MinValue>0</MinValue>
          <Align>Bottom</Align>
          <GridSpace>2</GridSpace>
          <MarkGridSpace>10</MarkGridSpace>
          <DataIndex>0</DataIndex>
        </SeriesInfo>
      </SeriesX>
      <SeriesY>
        <SeriesInfo>
          <Name>温度</Name>
          <Unit>℃</Unit>
          <LineColor>Blue</LineColor>
          <MaxValue>180</MaxValue>
          <MinValue>0</MinValue>
          <Align>Left</Align>
          <GridSpace>2</GridSpace>
          <MarkGridSpace>20</MarkGridSpace>
          <DataIndex>2</DataIndex>
        </SeriesInfo>
        <SeriesInfo>
          <Name>A0</Name>
          <Unit></Unit>
          <LineColor>Green</LineColor>
          <MaxValue>5000</MaxValue>
          <MinValue>0</MinValue>
          <Align>Right</Align>
          <GridSpace>50</GridSpace>
          <MarkGridSpace>1000</MarkGridSpace>
          <DataIndex>4</DataIndex>
        </SeriesInfo>
      </SeriesY>
    </CurveInfo>
  </CurvePrintConfig>

</config>
